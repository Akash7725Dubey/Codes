import java.lang.*;
import java.util.*;
class Main
{
    static int search(int x,int n,int a[])
    {
        for(int i=0;i<a.length;i++)
            if (a[i] == x)
                return i;

           return -1;

    }
    public static void main(String args[])
    {
        int a[]={1,2,3,4,5,6,7,8,9};
        int key=85;
        System.out.println(search(key,a.length,a));
    }

}
